{
	"name": "cs-api-authentication",
	"version": "0.0.11",
	"private": true,
	"description": "api wrappers for common account management functions",
	"main": "lib/index.js",
	"scripts": {
		"build": "babel src --out-dir lib",
		"test": "mocha --compilers js:babel-core/register test",
		"test:watch": "mocha --compilers js:babel-core/register --watch test",
		"commit": "npm run build && npm run test && git commit",
		"patch": "npm version patch",
		"push": "git push origin --follow-tags",
		"patch:push": "npm run build && npm run test && npm run patch && npm run push"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ControlSpaceSoftware/cs-api-authentication.git"
	},
	"author": "javadoug",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/ControlSpaceSoftware/cs-api-authentication/issues"
	},
	"homepage": "https://github.com/ControlSpaceSoftware/cs-api-authentication#readme",
	"devDependencies": {
		"build-scripts": "github:ControlSpaceSoftware/build-scripts.git"
	},
	"babel": {
		"presets": [
			"env"
		]
	}
}
